{
  "name": "replay-viewer",
  "version": "0.0.1",
  "description": "Rocket League replay viewer React component and tooling",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "node_modules/.bin/webpack --mode development --config webpack.config.js",
    "build:tsc": "npm run build:js && npm run build:defs",
    "build:js": "tsc --allowJs",
    "build:defs": "tsc --declaration --emitDeclarationOnly",
    "link": "npm link && cd examples && npm link replay-viewer && cd .."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SaltieRL/WebReplayViewer.git"
  },
  "keywords": [
    "Rocket",
    "League",
    "Replay",
    "React",
    "Three",
    "WebGL"
  ],
  "author": "SaltieRL",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SaltieRL/WebReplayViewer/issues"
  },
  "homepage": "https://github.com/SaltieRL/WebReplayViewer#readme",
  "dependencies": {
    "moment": "^2.24.0",
    "react": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@types/react": "^16.8.13",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.5",
    "source-map-loader": "^0.2.4",
    "typescript": "^3.4.3",
    "webpack": "4.28.3",
    "webpack-cli": "^3.3.0"
  }
}
