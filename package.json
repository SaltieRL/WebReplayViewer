{
  "name": "replay-viewer",
  "version": "0.8.1",
  "description": "Rocket League replay viewer React component and tooling",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "postinstall": "npm run submodule:init && cd docs && npm install",
    "clean": "rimraf lib",
    "lint": "eslint src/**/*.ts{,x}",
    "eslint:fix": "eslint --fix \"*.config.js\" \"src/**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \".eslintrc.js\" \"*.config.js\" \"tsconfig.json\" \"src/**/*.{ts,tsx,css}\"",
    "build": "npm run clean && npm run build:tsc && npm run build:bundle && npm run build:copy",
    "build:tsc": "tsc",
    "build:bundle": "webpack --progress",
    "build:copy": "node copy.js",
    "start": "cd docs && npm run start",
    "link": "npm link && cd docs && npm link replay-viewer",
    "submodule": "git submodule update --remote",
    "submodule:init": "git submodule update --init --remote",
    "publish:lib": "cd lib && npm publish",
    "prepublishOnly": "echo 'Do not publish from root. Publish from lib'; exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SaltieRL/WebReplayViewer.git"
  },
  "keywords": [
    "Rocket",
    "League",
    "Replay",
    "React",
    "Three",
    "WebGL"
  ],
  "author": "SaltieRL",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SaltieRL/WebReplayViewer/issues"
  },
  "homepage": "https://github.com/SaltieRL/WebReplayViewer#readme",
  "engines": {
    "node": "12.x"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.9.7",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "styled-components": "^5.2.1",
    "three": "^0.114.0"
  },
  "dependencies": {
    "lodash.debounce": "^4.0.8",
    "react-full-screen": "^1.0.1",
    "rl-loadout-lib": "^0.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.12.16",
    "@material-ui/core": "^4.9.7",
    "@types/lodash.debounce": "^4.0.6",
    "@types/node": "^14.14.28",
    "@types/react": "^16.13.0",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-dynamic-import-webpack": "^1.1.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "file-loader": "^6.2.0",
    "fs-extra": "^9.1.0",
    "prettier": "^2.2.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "rimraf": "^3.0.2",
    "styled-components": "^5.2.1",
    "three": "^0.114.0",
    "typescript": "^4.1.5",
    "webpack": "^5.22.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0"
  }
}
